#!/bin/sh
[ ! -f "/usr/share/ucitrack/luci-app-airconnect.json" ] && {
    cat > /usr/share/ucitrack/luci-app-airconnect.json << EEOF
{
    "config": "airconnect",
    "init": "airconnect"
}
EEOF
}

[ -f "/etc/config/ucitrack" ] && {
uci -q batch <<-EOF >/dev/null
EOF
}

rm -rf /tmp/luci-indexcache*
exit 0
